<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StarXpandSDK">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiscoveryActivity" />
        <activity android:name=".PrintingActivity" />
        <activity android:name=".MonitorActivity" />
        <activity android:name=".StatusActivity" />
        <activity android:name=".SpoolerActivity" />
        <activity android:name=".TemplatePrintActivity" />
        <activity android:name=".FirmwareUpdateActivity" />
    </application>

</manifest>